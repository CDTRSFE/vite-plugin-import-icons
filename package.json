{
    "name": "vite-plugin-import-icons",
    "version": "0.0.8",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.es.js",
    "homepage": "https://github.com/CDTRSFE/vite-plugin-import-icons",
    "repository": {
        "type": "git",
        "url": "https://github.com/CDTRSFE/vite-plugin-import-icons"
    },
    "scripts": {
        "dev": "rimraf dist && rollup -w -c",
        "build": "rimraf dist && rollup -c",
        "lint:eslint": "eslint \"src/**/*.{ts,js}\" --fix",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install",
        "release": "pnpm build && npm publish"
    },
    "keywords": [
        "vite",
        "vite-plugin",
        "icons"
    ],
    "author": "zhaoqing",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@trscd/eslint-config-ts": "^0.1.4",
        "@types/estree": "^1.0.0",
        "@types/node": "^18.6.1",
        "@vue/compiler-sfc": "^3.2.37",
        "acorn": "^8.8.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.20.0",
        "fast-glob": "^3.2.11",
        "fs-glob": "^1.1.23",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "magic-string": "^0.26.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.77.2",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-ts": "^3.0.2",
        "typescript": "^4.7.4",
        "vite": "^3.0.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts}": "pnpm lint:eslint"
    }
}
