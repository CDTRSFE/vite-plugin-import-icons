{
    "name": "vite-plugin-import-icons",
    "version": "0.0.12",
    "description": "",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "type": "module",
    "homepage": "https://github.com/CDTRSFE/vite-plugin-import-icons",
    "repository": {
        "type": "git",
        "url": "https://github.com/CDTRSFE/vite-plugin-import-icons"
    },
    "scripts": {
        "dev": "rimraf dist && rollup -w -c",
        "build": "rimraf dist && rollup -c",
        "lint:eslint": "eslint \"src/**/*.{ts,js}\" --fix",
        "prepare": "husky install",
        "release": "pnpm build && npm publish"
    },
    "keywords": [
        "vite",
        "vite-plugin",
        "icons"
    ],
    "author": "zhaoqing",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@trscd/eslint-config-ts": "^0.1.4",
        "@types/estree": "^1.0.5",
        "@types/node": "^20.12.2",
        "@vue/compiler-sfc": "^3.4.21",
        "acorn": "^8.11.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.0",
        "fast-glob": "^3.3.2",
        "fs-glob": "^1.1.23",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "magic-string": "^0.30.8",
        "rimraf": "^5.0.5",
        "rollup": "^4.13.2",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "typescript": "^5.4.3",
        "vite": "^5.2.7"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts}": "pnpm lint:eslint"
    }
}
